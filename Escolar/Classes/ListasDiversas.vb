    Public Function Pendencias() As Array
        Dim sql As String = "SELECT pendencia FROM `pendencias` ORDER BY pendencia;"
        Dim conn As MySqlConnection = dbMain.OpenDB(Globais.unidade, Globais.user, Globais.pwd, Globais.databaseName)
        Dim vrs As MySqlDataReader = dbMain.OpenTable(conn, sql)

        Dim lista As New List(Of String)
        If vrs.HasRows Then
            Try
                While vrs.Read
                    With lista
                        .Add(vrs.GetString("pendencia"))
                    End With
                End While
            Catch ex As Exception
            End Try
        End If
        dbMain.CloseAll(conn, vrs)
        Return lista.ToArray
    End Function

