    Public Function FatorSanguineo() As Array
        Dim fator As New List(Of String)
        With fator
            .Add("A+")
            .Add("A-")
            .Add("B+")
            .Add("B-")
            .Add("AB+")
            .Add("AB-")
            .Add("O+")
            .Add("O-")
        End With
        Return fator.ToArray
    End Function

    Public Function SimNao() As Array
        Dim escolha As New List(Of String)
        With escolha
            .Add("Não")
            .Add("Sim")
        End With
        Return escolha.ToArray
    End Function

