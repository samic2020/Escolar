Imports System.ComponentModel
Imports MySql.Data.MySqlClient

Public Class InitializeSystem
    Private dbMain As [Db] = New Db
    Private bSucesso As Boolean = False

    Public ReadOnly Property Sucesso() As Boolean
        Get
            Return bSucesso
        End Get
    End Property

    Private Sub btnCriar_Click(sender As Object, e As EventArgs) Handles btnCriar.Click
        '// Checa Servidor MySql
        Dim conn As MySqlConnection = dbMain.OpenDBMaster(iIpDns.Text.Trim, "root", iPassWord.Text.Trim)
        If conn Is Nothing Then
            Return
        End If
        dbMain.CloseDB(conn)

        Dim excBd As MsgBoxResult = MsgBox("Exclui o banco de dados caso exista?", MsgBoxStyle.Question + MsgBoxStyle.YesNo + MsgBoxStyle.DefaultButton2, "Atenção")
        If excBd = MsgBoxResult.Yes Then
            excBd = MsgBox("Tem certeza?", MsgBoxStyle.Question + MsgBoxStyle.YesNo + MsgBoxStyle.DefaultButton2)
        End If
        If excBd = MsgBoxResult.Yes Then
            dbMain.ExecuteCmdMaster("DROP SCHEMA IF EXISTS " & iDataBaseName.Text.Trim & ";")

            '// Cria o banco de daddos caso não existe       
            If Not dbMain.ExecuteCmdMaster("CREATE SCHEMA IF NOT EXISTS " & iDataBaseName.Text.Trim & " DEFAULT CHARACTER SET utf8 COLLATE utf8_bin;") Then
                MsgBox("Não foi possível criar o Banco de Dados " & iDataBaseName.Text.Trim & vbNewLine &
                           "Cheque as configurações e tente novamente",, "Atenção!")
                Return
            End If
        End If

        '// Checa se Banco de dados já esta criado
        Dim bTables As Boolean = False
        Dim resp As MsgBoxResult = MsgBox("Checa as tabelas do sistema?", MsgBoxStyle.Question + MsgBoxStyle.YesNo + MsgBoxStyle.DefaultButton2, "Atenção!")
        bTables = True

        If resp = MsgBoxResult.Yes Then
            Globais.unidade = iIpDns.Text.Trim
            Globais.databaseName = iDataBaseName.Text.Trim
            Globais.user = "root"
            Globais.pwd = iPassWord.Text.Trim
            CriarTabelas(bTables)

            '// Setar para 1G 
            dbMain.ExecuteCmd("SET GLOBAL max_allowed_packet=1073741824;")
            dbMain.ExecuteCmd("GRANT ALL PRIVILEGES ON *.* to 'root'@'%' IDENTIFIED BY @PASSWORD;", New Object()() {({"@PASSWORD", iPassWord.Text.Trim})})
            bSucesso = True
        End If

        Dispose()
    End Sub

